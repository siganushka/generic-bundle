{
    "name": "siganushka/generic-bundle",
    "description": "Generic bundle for symfony.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "siganushka",
            "email": "siganushka@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^6.4|^7.0"
    },
    "require-dev": {
        "composer/semver": "^3.0",
        "doctrine/doctrine-bundle": "^2.7",
        "friendsofphp/php-cs-fixer": "^3.0",
        "knplabs/knp-components": "^4.0",
        "siganushka/doctrine-contracts": "dev-main",
        "symfony/form": "^6.4|^7.0",
        "symfony/phpunit-bridge": "^6.4|^7.0",
        "symfony/serializer": "^6.4|^7.0",
        "symfony/translation": "^6.4|^7.0",
        "symfony/validator": "^6.4|^7.0",
        "vimeo/psalm": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Siganushka\\GenericBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Siganushka\\GenericBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true"
        }
    },
    "config": {
        "sort-packages": true
    }
}
